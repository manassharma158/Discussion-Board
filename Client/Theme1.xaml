<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:local="clr-namespace:Client">

    <Color x:Key="Color1">#FF340051</Color>
    <Color x:Key="Color2">#FF8522BD</Color>
    <Color x:Key="Color3">#FFCFCFCF</Color>

    <SolidColorBrush x:Key="Color1Bg" Color="#FF340051"></SolidColorBrush>
    <SolidColorBrush x:Key="Color2Bg" Color="#FF8522BD"></SolidColorBrush>
    <SolidColorBrush x:Key="Color3Bg" Color="#FFCFCFCF"></SolidColorBrush>


</ResourceDictionary>